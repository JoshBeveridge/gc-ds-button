    "np": "^3.1.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "typescript": "^3.3.4000"
  },
  "dependencies": {
  },
  "peerDependencies": {
    "react": "^17.0.2",
    "react-dom": "^17.0.2"
